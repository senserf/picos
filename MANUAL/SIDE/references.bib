@ARTICLE{sag94,
AUTHOR="Stamoulis, G. D. and Anagnostou, M. E. and Georgantas, A. D.",
TITLE="Traffic source models for {ATM} networks: a survey",
JOURNAL={Computer Communications},
VOLUME=17,
NUMBER=6,
PAGES="428-438",
MONTH=jun,
YEAR=1994,
REFERENCES=26,
ABSTRACT="Selecting appropiate models of ATM traffic sources is an
important issue, since it is closely related to the successful design
and efficient performance of the ATM networks to be built in the future.
For the three basic categories of sources -voice, data and video-,
numerous source models have been proposed in the literature. In this
paper, we summarize the main features of each of these categories, and
present a survey of related models. We argue that it is preferable to
select a set of standard source models rather than a single such model.
We define a set of criteria to be used in the selection, and choose
appropiate source models accordingly.",
KEYWORDS="traffic source models; selection criteria; ATM",
ENTRYBY=Do
}

@INPROCEEDINGS{tag94,
AUTHOR="Tassiulas, L. and Georgiadis, L.",
TITLE="Any Work-Conserving Policy Stabilizes the Ring with Spatial Reuse",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
MONTH=jun,
PAGES="66--70",
YEAR=1994,
ABSTRACT="We consider the ring network with spatial reuse. Traffic
streams may enter and exit the network at any node. The burstiness of
each traffic stream is bounded by a deterministic bound. A transmission
policy specifies at each time which traffic stream will be transmitted
at the outgoing link by each node. We provide an upper bound on the
asymptotic backlog of the ring that holds for all work- conserving
policies and is independent of the initial conditions. This bound
remains finite as long as the maximum load of every link is less than
one. The latter condition is also necessary for the existence of an
asymptotic bound that is independent of the initial conditions.",
KEYWORDS="Stability; Work-conserving policies; Ring",
ENTRYBY=Do
}

@INPROCEEDINGS{brm94,
AUTHOR="Breuer, Simone and Meuser, Thomas",
TITLE="Enhanced Throughput in Slotted Rings Employing Spatial Slot Reuse",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
MONTH=jun,
PAGES="1120--1129",
YEAR=1994,
ABSTRACT="A cell based MAC protocol provides both efficient bandwidth
utilisation and easy interfacing with the emerging ATM technology.
Therefore the slotted approach is being favoured for data transmission
and admission control in distributed LANs operating at data rates of
more than one Gbit/s. Spatial slot reuse through destination release has
been found to be the most effective way towards improving system
throughput. That basic mechanism, however, is unfair by giving
preference to stations positioned upstream in the ring circulation
direction. This contribution discusses approaches for an asynchronous
media access control in slotted rings. After a general overview of
system throughput obtained by an analytical model, the feasibility of
various MAC proposals called ATMR, CRMA-I, Distributor, and MetaRing are
compared based on simulation results. Differences in the mechanisms are
analysed with a particular respect to the tradeoff between high
throughput and the guarantee of bounded packet delays. It is shown, that
all approaches are generally suitable to meet the requirements of the
new LAN generation.",
KEYWORDS="Local Area Networks; Performance Evaluation; Network Protocols",
ENTRYBY=Do
}

@INPROCEEDINGS{shs94,
AUTHOR="Sharon, Oran and Segall, Adrian",
TITLE="On the Efficiency of Slot Reuse in the Dual Bus Configuration",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
MONTH=jun,
PAGES="758--765",
YEAR=1994,
ABSTRACT="Slot reuse is a simple method to improve the efficiency of
MACs designed for the slotted, Dual Bus configuration. In this paper we
propose several schemes for the assignment of throughput to stations in
this configuration, taking into account slot reuse and assuming uniform
traffic. The criteria for comparison of the schemes are throughput gain,
worst message queueing and access delays at stations and the fraction of
blocked messages. It turns out that the throughput gain in the scheme in
which every station receives the same throughput is relatively low but
it is the most attractive scheme is relation to the other criteria.",
ENTRYBY=Do
}

@INPROCEEDINGS{cgg94,
AUTHOR="Cidon, I. and Georgiadis, L. and Guerin, R. and Shavitt, Y.",
TITLE="Improved Fairness Algorithms for Rings with Spatial Reuse",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
PAGES="1103--1111",
MONTH=jun,
YEAR=1994,
ABSTRACT="Improves SAT.
Ring network architectures that employ spatial reuse permit
concurrent transmissions of messages over different links. While spatial
reuse increases network throughput, it may also cause starvation of
nodes. To alleviate this problem, various policies have been suggested
in the literature. In this paper we concentrate on a class of such
policies that achieve fairness by allocating transmission quotas to
nodes. For such policies, we provide mechanisms for improving delays and
increasing overall throughput without compromising fairness.",
KEYWORDS="Local Area Networks; Ring Networks; Spatial Reuse; Fair
Policies; Delay ;Throughput Performance",
ENTRYBY=Do
}

@INPROCEEDINGS{wis94,
AUTHOR="Willebeek-LeMair, Marc and Shahabuddin, Perwez",
TITLE="Approximating Dependability Measures of {FDDI} Networks",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
PAGES="1372--1381",
MONTH=jun,
YEAR=1994,
ABSTRACT="An approximation technique is presented to evaluate the
dependability of computer networks. This technique, based on the most
likely paths to failure, is simple and practical. It may easily be
applied by a network designer in evaluating and comparing different
network configurations. The approximation technique is applied to a
variety of FDDI network configurations. The effect of certain network
parameters on the network availability are examined. From these results
we conclude that, in order to guarantee high availability, an FDDI
network backbone should be interconnected using dual attachment
concentrators. Furthermore, dual-homing configurations are required for
high availability paths between end-stations and the backbone. For less
stringent availability requirements, single attachment concentrator
trees with single attachment stations may suffice.",
KEYWORDS="Dependability; Network; FDDI; Failure Paths",
ENTRYBY=Do
}

@INPROCEEDINGS{yis94,
AUTHOR="Yin, Jiahnsheng and Silio, Jr., Charles B.",
TITLE="Reliability of {FDDI}'s Dual Homing Network Architecture",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
PAGES="1382--1389",
MONTH=jun,
YEAR=1994,
ABSTRACT="We analyze the reliability of two FDDI computer communication
network architectures and make comparisons; these structures are a
fault-tolerant dual-homing ring of trees and a failsoft ring of trees.
We calculate the probability P[h(s,t)] that two arbitrarily positioned
terminal stations (s and t) can communicate by deriving closed-form
polynomial expressions for P[h(s,t)] in terms of component failure
probabilities. The new expression for dual homing permits efficient
quantitative reliability comparis- ons for networks with an arbitrary
number of terminal stations, an arbitrary number of concentrator tree
root pairs in the counter rotating dual ring, and varying numbers of
ports in the concentrators.",
KEYWORDS="Fault-tolerance; ring networks; dual homing; FDDI; token
rings; failsoft; ring of trees; reliability; availability",
ENTRYBY=Do
}

@INPROCEEDINGS{hel94,
AUTHOR="Heyman, D. P. and Lakshman, T. V.",
TITLE="Source Models for {VBR} Broadcast-Video Traffic",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
PAGES="664--671",
MONTH=jun,
YEAR=1994,
ABSTRACT="Traffic from video services is expected to be a substantial
portion of the traffic carried by emerging broadband integrated
networks. For variable bit rate (VBR) coded video, statistical source
models are needed to design networks that achieve acceptable picture
quality at minimum cost, and to design traffic shaping and control
mechanisms. For video teleconference traffic, we have shown (in a prior
paper) that traffic is sufficiently accurately characterized by a
multi-state Markov chain model that can be derived from three traffic
parameters (mean, correlation, and variance). In this paper, we describe
modeling results for sequences with frequent scene changes (the
previously studied video teleconferences have very little scene
variation) such as entertainment television, news, and sports
broadcasts. We analyze eleven long sequences of broadcast video traffic
data. Unlike video teleconferences, the different sequences studied here
have different details regarding distributions of cells per frame. We
present source models applicable to the different sequences and evaluate
their accuracy as predictors of cell losses in Asynchronous Transfer
Mode (ATM) networks.",
KEYWORDS="Packet video; asynchronous transfer mode networks; traffic
models; bandwidth allocation; congestions avoidance",
ENTRYBY=Do
}

@INPROCEEDINGS{alv94,
AUTHOR="Ali, Irfan and Vastola, Kenneth S.",
TITLE="The Shifting Cycle-Gated {SCG} Protocol for High Speed Bus
Networks",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
PAGES="1112--1119",
ADDRESS="Toronto, Canada",
MONTH=jun,
YEAR=1994,
ABSTRACT="U-Net+H-Net.
The exhaustive cycle-gated (ECG) scheme was proposed as a high
throughput access mechanism for high speed bus protocols like Expressnet
and Fasnet. However, the ECG scheme exhibits unfairness to downstream
station which suffer higher mean and variance of waiting time. In this
paper we propose and analyze a new cycle-gated scheme, the shifting
cycle-gated (SCG) scheme, on the Expressnet topology. We demonstrate
that the SCG scheme is fair to all the stations in the sense that the
mean and variance of the waiting time are the same for all stations,
irrespective of the spatial distribution of the stations on the bus and
the service time distribution of the packets at the stations, for
uniform load. The SCG scheme achieves this fairness by shifting the
order of service of stations from one cycle to the next. Our analysis
methodology uses space-time diagrams to derive expressions for the
moment generating function, mean and variance of the waiting time under
assumptions of independent nonsymmetric Poisson arrival processes at N
stations on the bus. Extensive numerical results illustrate the behavior
of the SCG protocol.",
KEYWORDS="high speed protocols; performance evaluation; bus networks;
multiple access",
ENTRYBY=Do
}

@INPROCEEDINGS{kaz94,
AUTHOR="Kamat, Sanjay and Agrawal, Gopal and Zhao, Wei",
TITLE="Available Bandwidth in {FDDI-Based} Reconfigurable Networks",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
MONTH=jun,
PAGES="1390--1397",
YEAR=1994,
ABSTRACT="Networks used in mission-critical applications must be highly
fault-tolerant. We propose an FDDI-Based Reconfigurable Network (FBRN)
that can survive multiple faults. An FBRN consists of multiple FDDI
trunk rings and has the ability to reconfigure itself in the face of
extensive damage. We consider three classes of FBRNs depending on their
degree of reconfigurability: N-FBRN (non- reconfigurable), P-FBRN
(partially reconfigurable) and F- FBRN (fully reconfigurable). We
analyze the performance of FBRNs in terms of their available bandwidth
in the presence of faults. In mission-critical systems, it is not enough
to ensure that the network is connected in spite of faults; the traffic
carrying capacity of the network must be sufficient to guarantee the
required quality of service. We present a probabilistic analysis of the
available bandwidth of FBRNs given the number of faults. We find that a
fully reconfigurable FBRN can provide a high available bandwidth even in
the presence of a large number of faults. A partially reconfigurable
FBRN is found to be an excellent compromise between high reliability and
ease of implementation.",
KEYWORDS="FDDI; Fault Tolerance; Available Bandwidth; Reconfigurable
Networks; Network Management",
ENTRYBY=Do
}

@INPROCEEDINGS{hwm94,
AUTHOR="Hassanein, H. S. and Wong, J. W. and Mark, J. W.",
TITLE="An Effective Erasure Node Algorithm for Slot Reuse in {DQDB}",
BOOKTITLE={Proceedings of IEEE INFOCOM'94},
ADDRESS="Toronto, Canada",
PAGES="1302--1309",
MONTH=jun,
YEAR=1994,
ABSTRACT="The Distributed Queue Dual Bus (DQDB) protocol, which has been
specified by the IEEE 802.6 as the Metropolitan Area Networks (MANs)
standard, has an undesirable feature that busy slots that has been
already received (i.e. read) continue to propagate downstream
unnecessarily. The use of special nodes, known as erasure nodes, allows
conversion of read slots to empty, so that downstream nodes may reuse
these slots. This should result in an increase of the total throughput
of the network. In this paper, we introduce and evaluate the performance
of an erasure node algorithm. The algorithm keeps track of past
activities on both busses to effectively balance the slot erasure and
the request cancellation functions. We show that this algorithm
possesses a number of features which make it superior to existing
algorithms.",
KEYWORDS="DQDB; spatial reuse; erasure nodes; performance evaluation;
MAN",
ENTRYBY=Do
}

