@INPROCEEDINGS{www94,
AUTHOR={Walch, Markus and Wolisz, Adam and Wolf-G\"unther, J.},
TITLE="Visualization and Performance Analysis of Formally Specified
Communication Protocols",
BOOKTITLE="Proceedings of MASCOTS'94",
ADDRESS="Durham, North Carolina, USA",
PAGES="284--291",
MONTH=jan,
YEAR=1994,
ABSTRACT="There is an increasing interest in developing
performance-enhanced extensions of standardised FDTs. In this paper we
present how simulation and visualization of performance-extended formal
specifications can be efficiently supported by the use of a high-level
simulation environment. Requirements for selection of a proper
environment are formulated, and the design principles for a software
package, build on top of this environment are presented. The developed
software supports both editing and animated stepwise execution of the
specifications using peculiar concepts like: actions, processes,
operators etc. In addition the execution of simulation with collection
of statistical data and deadlock detection, as well as graphical
presentation of results are supported.",
KEYWORDS="formal methods; protocol specification; TIS; software tools",
URL="ftp://ftp.fokus.gmd.de/pub/docs/minos/mwa-Visualization-Mascots94.ps",
ENTRYBY=Sc
}

